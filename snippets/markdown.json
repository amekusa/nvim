{
  "</small>": {
    "body": "</small>",
    "prefix": "</small>"
  },
  "<kbd>...</kbd>": {
    "body": "<kbd>$1</kbd>$0",
    "prefix": "<kbd>...</kbd>"
  },
  "<small>": {
    "body": "<small>",
    "prefix": "<small>"
  },
  "<tag class=\"...\">...</tag>": {
    "body": "<${1:div} class=\"$2\">$3</${1:div}>$0",
    "prefix": "<tag class=\"...\">...</tag>"
  },
  "<tag>...</tag>": {
    "body": "<${1:div}>$2</$1>$0",
    "prefix": "<tag>...</tag>"
  },
  "marp: class": {
    "body": "<!-- _class: $1 -->",
    "prefix": "marp: class"
  },
  "md: table 2 cols": {
    "body": [
      "| $1 | $2 |",
      "| :-- | :-- |",
      "| $0 |  |",
      "|  |  |"
    ],
    "prefix": "md: table 2 cols"
  }
}
