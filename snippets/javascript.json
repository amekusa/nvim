{
  "#!/usr/bin/env node + :shebang": {
    "body": "#!/usr/bin/env node",
    "prefix": [
      "#!/usr/bin/env node",
      ":shebang"
    ]
  },
  ".then(_ => { ... })": {
    "body": ".then($1 => {})",
    "prefix": ".then(_ => { ... })"
  },
  "/** :docblock */": {
    "body": [
      "/**",
      " * $1",
      " */"
    ],
    "prefix": "/** :docblock */"
  },
  ":arrow": {
    "body": "$1 => {$0}",
    "prefix": ":arrow"
  },
  ":arrow2": {
    "body": "($1) => {$0}",
    "prefix": ":arrow2"
  },
  ":for-array": {
    "body": "for (let i = ${1:0}; i < ${2:arr}.length; i++) $0",
    "prefix": ":for-array"
  },
  "@param {_} _ - _": {
    "body": "@param {${1:type}} ${2:name} - ${3:desc}",
    "prefix": "@param {_} _ - _"
  },
  "@return {_} _": {
    "body": "@return {${1:type}}${2: desc}",
    "prefix": "@return {_} _"
  },
  "console.debug(_) + :debug": {
    "body": "console.debug($1);",
    "prefix": [
      "console.debug(_)",
      ":debug"
    ]
  },
  "console.log(_) + :log": {
    "body": "console.log($1);",
    "prefix": [
      "console.log(_)",
      ":log"
    ]
  },
  "const _ = _": {
    "body": "const $1 = $2;",
    "prefix": "const _ = _"
  },
  "const _ = require('_') + :require": {
    "body": "const $1 = require('$2');",
    "prefix": [
      "const _ = require('_')",
      ":require"
    ]
  },
  "constructor(_) { ... }": {
    "body": [
      "constructor(${1:}) {",
      "\t$0",
      "}"
    ],
    "prefix": "constructor(_) { ... }"
  },
  "export _": {
    "body": "export $0;",
    "prefix": "export _"
  },
  "export default _": {
    "body": "export default ${1:};",
    "prefix": "export default _"
  },
  "function _(_) { ... }": {
    "body": "function $1($2) {$0}",
    "prefix": "function _(_) { ... }"
  },
  "import _ from '_'": {
    "body": "import $1 from '$2';",
    "prefix": "import _ from '_'"
  },
  "let _ = _": {
    "body": "let $1 = $2;",
    "prefix": "let _ = _"
  },
  "switch (typeof _) { ... }": {
    "body": [
      "switch (typeof $1) {",
      "case '$2':",
      "\t$0",
      "\tbreak;",
      "}"
    ],
    "prefix": "switch (typeof _) { ... }"
  }
}
